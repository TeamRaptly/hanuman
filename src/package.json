{
  "name": "hanuman-client",
  "description": "Hanuman client app",
  "scripts": {
    "createfirebaseconf": "firebase setup:web --json > ../functions/firebase-config.json",
    "buildClient": "webpack --config ./webpack.client.js",
    "buildServer": "webpack --config ./webpack.server.js",
    "devClient": "webpack --config ./webpack.client.js --watch",
    "devServer": "webpack --config ./webpack.server.js --watch",
    "build": "npm run buildClient && npm run buildServer"
  },
  "dependencies": {
    "axios": "0.19.2",
    "firebase": "7.9.2",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "6.0.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "babel-loader": "8.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  }
}
