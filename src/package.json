    "react-dom": "16.13.0",
    "react-helmet": "6.0.0",
    "react-redux": "6.0.1",
    "react-router": "5.1.2",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-batched-actions": "0.5.0",
    "redux-thunk": "2.3.0",
    "styled-components": "4.4.1"
