{
  "name": "hanuman-client",
  "description": "Hanuman client app",
  "main": "index.js",
  "scripts": {
    "build-client": "npm run clean-client-build && webpack --config ./webpack.client.js",
    "build-server": "npm run clean-server-build && webpack --config ./webpack.server.js",
    "build-dev-client": "npm run clean-client-build && webpack --config ./webpack.client.js --watch",
    "build-dev-server": "npm run clean-server-build && webpack --config ./webpack.server.js --watch",
    "build": "npm run build-client && npm run build-server",
    "dev-server": "nodemon index.js",
    "lint:css": "stylelint './**/*.js'",
    "lint:js": "eslint --max-warnings=0 *.js",
    "test": "jest",
    "start": "node index.js",
    "analyze-client-bundle": "node tools/bundle-analyze-client.js",
    "analyze-server-bundle": "node tools/bundle-analyze-server.js",
    "clean-server-build": "rimraf \"./build/**\"",
    "clean-client-build": "rimraf \"./public/**\""
  },
  "dependencies": {
    "@babel/core": "7.10.2",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-decorators": "7.10.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.10.1",
    "@babel/plugin-proposal-optional-chaining": "7.10.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-classes": "7.10.1",
    "@babel/plugin-transform-react-constant-elements": "7.10.1",
    "@babel/plugin-transform-react-inline-elements": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "@babel/register": "7.9.0",
    "@loadable/component": "5.12.0",
    "@material-ui/core": "4.10.1",
    "axios": "0.19.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-plugin-webpack-alias": "2.1.2",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "core-js": "3.6.5",
    "express": "4.17.1",
    "express-handlebars": "4.0.4",
    "firebase": "7.9.2",
    "firebase-admin": "8.9.2",
    "isomorphic-fetch": "2.2.1",
    "merge": "1.2.1",
    "prop-types": "15.7.2",
    "ramda": "0.27.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "6.0.0",
    "react-redux": "6.0.1",
    "react-router": "5.1.2",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-batched-actions": "0.5.0",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.5",
    "rimraf": "3.0.2",
    "serialize-javascript": "3.0.0",
    "styled-components": "4.4.1",
    "webpack": "4.43.0",
    "webpack-node-externals": "1.7.2"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/plugin-transform-arrow-functions": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@loadable/babel-plugin": "5.12.0",
    "@loadable/webpack-plugin": "5.12.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-import": "1.13.0",
    "babel-plugin-remove-webpack": "1.1.0",
    "babel-plugin-styled-components": "1.10.7",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "eslint": "6.8.0",
    "eslint-plugin-promise": "4.2.1",
    "jest": "26.0.1",
    "jest-styled-components": "7.0.2",
    "nodemon": "2.0.4",
    "source-map-support": "0.5.19",
    "stylelint": "13.5.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-custom-processor-loader": "0.6.0",
    "stylelint-processor-styled-components": "1.10.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.11"
  }
}
