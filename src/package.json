{
  "name": "hanuman-client",
  "description": "Hanuman client app",
  "scripts": {
    "createfirebaseconf": "firebase setup:web --json > ../functions/firebase-config.json",
    "buildClient": "webpack --config ./webpack.client.js",
    "buildServer": "webpack --config ./webpack.server.js",
    "devClient": "webpack --config ./webpack.client.js --watch",
    "devServer": "webpack --config ./webpack.server.js --watch",
    "build": "npm run buildClient && npm run buildServer",
    "lint:css": "stylelint './**/*.js'",
    "test": "jest",
    "analyze-client-bundle": "node tools/bundle-analyze-client.js",
    "analyze-server-bundle": "node tools/bundle-analyze-server.js"
