{
  "name": "hanuman",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "10"
  },
  "watch": {
    "dev-server": "src/functions/**/*.js"
  },
  "scripts": {
    "dev": "next \"src/app/\"",
    "preserve": "npm run clean && npm run build-public && npm run build-funcs && npm run build-app && npm run copy-deps && npm run install-deps",
    "preserve-only": "npm run build-public && npm run build-funcs && npm run build-app",
    "serve": "npm run preserve && cross-env NODE_ENV=production firebase serve",
    "predeploy": "npm run build-public && npm run build-funcs && npm run build-app && npm run copy-deps",
    "deploy": "NODE_ENV=production firebase deploy",
    "clean": "rimraf \"dist/functions/**\" && rimraf \"dist/public\"",
    "build-public": "cpx \"src/app/public/**/*.*\" \"dist/public\" -C",
    "build-funcs": "babel \"src/functions\" --out-dir \"dist/functions\" --copy-files",
    "build-app": "next build \"src/app/\"",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i",
    "dev-server": "npm run preserve-only && cross-env NODE_ENV=production firebase serve --only functions",
    "dev-server-watch": "npm-watch dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vishwasc20/hanuman.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vishwasc20/hanuman/issues"
  },
  "homepage": "https://github.com/vishwasc20/hanuman#readme",
  "dependencies": {
    "@babel/runtime": "7.9.2",
    "@material-ui/core": "4.9.11",
    "@material-ui/icons": "4.9.1",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "firebase-admin": "8.11.0",
    "firebase-functions": "3.6.1",
    "isomorphic-fetch": "2.2.1",
    "merge": "1.2.1",
    "next": "9.3.5",
    "ramda": "0.27.0",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "cpx": "1.5.0",
    "cross-env": "7.0.2",
    "firebase-functions-test": "0.2.1",
    "firebase-tools": "8.1.1",
    "npm-watch": "0.6.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2"
  }
}
